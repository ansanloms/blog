{
  "name": "blog",
  "version": "1.0.0",
  "description": "blog",
  "author": "ansanloms",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "lint": "run-s lint:*",
    "lint:textlint": "textlint ./content/**",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "fix": "run-s fix:*",
    "fix:textlint": "textlint --fix ./content/**",
    "fix:eslint": "eslint --ignore-path .gitignore --fix .",
    "fix:prettier": "prettier --write --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ansanloms/blog.git"
  },
  "dependencies": {
    "dayjs": "^1.10.5",
    "destyle.css": "^2.0.2",
    "gray-matter": "^4.0.3",
    "next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^6.0.2",
    "react-syntax-highlighter": "^15.4.3",
    "rehype-raw": "^5.1.0",
    "remark-gfm": "^1.0.0",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@textlint-ja/textlint-rule-no-synonyms": "^1.1.0",
    "@types/node": "^15.12.3",
    "@types/react": "^17.0.11",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "sudachi-synonyms-dictionary": "^8.0.0",
    "textlint": "^12.0.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-no-mix-dearu-desumasu": "^5.0.0",
    "textlint-rule-preset-ja-spacing": "^2.1.3",
    "textlint-rule-preset-ja-technical-writing": "^6.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "typescript": "^4.3.4"
  }
