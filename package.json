{
  "name": "blog",
  "version": "1.0.0",
  "description": "blog",
  "author": "ansanloms",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "lint": "run-p --continue-on-error lint:*",
    "lint:textlint": "textlint ./content/**",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "fix": "run-p --continue-on-error fix:*",
    "fix:textlint": "textlint --fix ./content/**",
    "fix:eslint": "eslint --ignore-path .gitignore --fix .",
    "fix:prettier": "prettier --write --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ansanloms/blog.git"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "destyle.css": "^3.0.2",
    "gray-matter": "^4.0.3",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^8.0.0",
    "react-syntax-highlighter": "^15.4.5",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "@textlint-ja/textlint-rule-no-synonyms": "^1.3.0",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "sudachi-synonyms-dictionary": "^10.0.0",
    "textlint": "^12.1.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-no-mix-dearu-desumasu": "^5.0.0",
    "textlint-rule-preset-ja-spacing": "^2.2.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "typescript": "^4.5.5"
  }
}
